<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.xishangqing.dao.UserDao">
    <select id="selectUsers" resultType="User">
        select id,name,emile,age from user order by
    </select>

    <insert id="insertUser">
        insert into user(name, emile, age) values (#{name}, #{emile}, #{age})
    </insert>


</mapper>


